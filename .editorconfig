# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = tab
insert_final_newline = false
tab_width = 4
trim_trailing_whitespace = true

[*.md]
indent_size = 2
indent_style = space
tab_width = 2
trim_trailing_whitespace = false
